{
  "name": "node_homeworks_15",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npx nodemon",
    "build": "npx tsc",
    "start": "npm run build && node build/index",
    "eslint": "eslint",
    "format": "npx eslint ./src --fix",
    "db:seed": "ts-node ./src/dal/seed/index.ts",
    "db:migrate-up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d ./src/dal/dataSource.ts",
    "db:migrate-down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/dal/dataSource.ts",
    "db:migrate-create": "npx typeorm migration:create ./src/dal/migrations/columnDeleted",
    "db:migrate-update": "npx typeorm migration:generate ./src/dal/migrations/columnDeleted -d ./src/dal/dataSource.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sentry/integrations": "^7.49.0",
